{
    "terminal.integrated.rendererType": "dom",
    "workbench.iconTheme": "eq-material-theme-icons-ocean",
    "editor.fontFamily": "'Source Code Pro', 'monospace', monospace",
    "todo-tree.tree.showScanModeButton": true,
	"todo-tree.highlights.defaultHighlight": {
		"icon": "alert",
		"type": "text",
		"foreground": "red",
		"background": "#1A2B38",
		"opacity": 50,
		"iconColour": "red"
	},
	"todo-tree.highlights.customHighlight": {
		"TODO": {
			"foreground": "#FF7E6B",
			"iconColour": "#FF7E6B",
			"icon": "issue-opened",
			"type": "tag",
			"gutterIcon": true
		},
		"FIXME": {
			"foreground": "#FFE548",
			"background": "#FF000000",
			"iconColour": "#FFE548",
			"gutterIcon": true,
			"type": "line"
		},
		"NOTE": {
			"foreground": "#FEFDFF",
			"background": "#FF000000",
			"gutterIcon": false,
			"type": "line",
			"hideFromTree": true
		},
		"$": {
			"foreground": "#F96900",
			"background": "#00000070",
			"gutterIcon": false,
			"type": "line",
			"hideFromTree": true
		},
		"[JIRA]": {
			"foreground": "#97CCD8",
			"background": "#FF000000",
			"gutterIcon": true,
			"icon": "meter",
			"iconColour": "#97CCD8",
			"type": "line",
			"hideFromTree": true
		},
		"EMPHASIS": {
			"foreground": "#9EBE0E",
			"gutterIcon": false,
			"type": "line",
			"hideFromTree": true
		},
		"bullet": {
			"foreground": "#EF798A",
			"type": "tag",
			"hideFromTree": true,
			"gutterIcon": false
		}
	},
	"todo-tree.general.tags": [
		"BUG",
		"HACK",
		"FIXME",
		"TODO",
		"todo",
		"fix",
		"fixme",
		"fixthis",
		"hack",
		"bug",
		"FIX ME",
		"NOTE",
		"note",
		"NOTA",
		"nota",
		"$",
		"[JIRA]",
		"EM",
		"EMPHASIS",
		"bullet",
		">",
		"-->",
		"-",
		"->",
		"[ ]",
		"[x]"
	],
	"todo-tree.general.tagGroups": {
		"TODO": [
			"TODO",
			"todo"
		],
		"FIXME": [
			"FIXME",
			"fix",
			"fixme",
			"fixthis",
			"FIX ME"
		],
		"NOTE": [
			"NOTE",
			"note",
			"NOTA",
			"nota"
		],
		"[JIRA]": [
			"[JIRA]",
			"[Jira]"
		],
		"EMPHASIS": [
			"EMPHASIS",
			"EM"
		],
		"bullet": [
			"bullet",
			">",
			"-->",
			"-",
			"->"
		]
	},
    "C_Cpp.updateChannel": "Insiders",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "java.semanticHighlighting.enabled": true,
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "workbench.colorTheme": "Dracula",
    "python.jediEnabled": false,
    "java.imports.gradle.wrapper.checksums": [
        {
            "sha256": "4160d5a6d8d6efc6af336582bbbba8194e4d7a742835f7b0fd3964cbd419c994",
            "allowed": true
        },
        {
            "sha256": "778e7f46bd67eaea2de5bcbdbb40878c6614656014ba59a72ce8648eaf43a925",
            "allowed": true
        }
    ],
    "python.languageServer": "Pylance",
    "python.showStartPage": false,
	"javascript.updateImportsOnFileMove.enabled": "always",
	"todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
	"diffEditor.ignoreTrimWhitespace": false
}